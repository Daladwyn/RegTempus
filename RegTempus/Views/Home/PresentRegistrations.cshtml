@model List<RegTempus.ViewModels.PresentRegisteredTimeViewModel>

@{
    ViewData["Title"] = "PresentRegistrations";
    Layout = "~/Views/_Layout.cshtml";
}

<ul class="nav">
    <li class="nav-item">
        <form asp-controller="Home" asp-action="Index" method="post">
            <input class="btn btn-primary" type="submit" name="name" value="Time registration" />
        </form>
    </li>
    <li class="nav-item">
        @ViewBag.Month
    </li>
</ul>

<div class="row">
    <div class="col-2 col-sm-2 col-md-2">
        <span>Day</span>
    </div>
    <div class="col-2 col-sm-2 col-md-2">
        <span>Start</span>
    </div>
    <div class="col-2 col-sm-2 col-md-2">
        <span>Stop</span>
    </div>
    <div class="col-2 col-sm-2 col-md-2">
        <span>Break</span>
    </div>
    <div class="col-4 col-sm-4 col-md-4">
    </div>
</div>
@foreach (var day in Model)
{

    <div class="row">
        <div class="col-2 col-sm-2 col-md-2">
            @day.Day
        </div>
        <div class="col-2 col-sm-2 col-md-2">
            <div id="">@day.TimeStart.ToShortTimeString()</div>
            <button class=""></button>
        </div>
        <div class="col-2 col-sm-2 col-md-2">
            <div>@day.TimeStop.ToShortTimeString()</div>
        </div>
        <div class="col-2 col-sm-2 col-md-2">
            <div>@day.TimeBreakString</div>
        </div>
        <div class="col-4 col-sm-4 col-md-4">
        </div>
    </div>
}


